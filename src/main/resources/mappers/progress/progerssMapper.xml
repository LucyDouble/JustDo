<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="Progress">

<update id="">
	<selectKey resultType="Progress" keyProperty="beforePro" order="BEFORE" >
		select progress from progress where progress_no = #{progress_no}
	</selectKey>
</update>

</mapper>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting value="NULL" name="jdbcTypeForNull"/>
	</settings>
	<typeAliases>
		<typeAlias type="com.kh.jd.account.Student" alias="Student"/>
		<typeAlias type="com.kh.jd.account.Teacher" alias="Teacher"/>
		<typeAlias type="com.kh.jd.exam.Exam" alias="Exam"/>
		<typeAlias type="com.kh.jd.work.Work" alias="Work"/>
		<typeAlias type="com.kh.jd.attend.Attend" alias="Attend"/>
		<typeAlias type="com.kh.jd.lecture.Lecture" alias="Lecture"/>
	</typeAliases>
	<mappers>
		<mapper resource="mappers/account/studentMapper.xml"/>		
		<mapper resource="mappers/account/teacherMapper.xml"/>		
		<mapper resource="mappers/attend/attendMapper.xml"/>		
		<mapper resource="mappers/exam/examMapper.xml"/>		
		<mapper resource="mappers/work/workMapper.xml"/>
		<mapper resource="mappers/lecture/lectureMapper.xml"/>		
	</mappers>
</configuration>
